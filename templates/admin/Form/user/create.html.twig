{# templates/vehicle/create.html.twig #}

<!DOCTYPE html>
<html>
	<head>
		<title>Ajouter un User</title>
	</head>
	<body>

		<h1>Ajouter un User</h1>

		{{ form_start(form) }}
		{{ form_row(form.pseudo) }}
		{{ form_row(form.password) }}
		{{ form_row(form.nom) }}
		{{ form_row(form.prenom) }}
		{{ form_row(form.email) }}
		{{ form_row(form.civilite) }}
		{{ form_end(form) }}

	</body>
</html>
